<mapper namespace="com.application.utility.practice1.chapter04_scheduler.SchedulerDAO">

	<!-- 상품 전체조회 -->
	<select id="getProductList" resultType="ProductDTO">
		SELECT	*
		FROM	PRODUCT
	</select>
	
	<!-- 브랜드 전체조회 -->
	<select id="getBrandList" resultType="BrandDTO">
